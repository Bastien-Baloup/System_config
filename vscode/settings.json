  "editor.fontFamily": "'Fira Code Anonymous','FiraCode Nerd Font Mono', 'Droid Sans Mono', 'monospace', monospace, 'Droid Sans Fallback'",
  "editor.tokenColorCustomizations": {
    "textMateRules": [
      {
        "scope": [
          //following will be in italic
          "comment",
          "constant.language",
          "entity.other.attribute-name",
          "string.quoted",
          "meta.attribute-selector",
          "keyword.other"
        ],
        "settings": {
          "fontStyle": "italic"
        }
      },
      {
        "scope": [
          //following will be excluded from italics
          "invalid",
          "keyword.operator",
          "constant.numeric.css",
          "constant.numeric.decimal.js",
          "constant.numeric.json",
          "support.class.component.html"
        ],
        "settings": {
          "fontStyle": ""
        }
      },
      {
        "scope": [
          "storage",
          "keyword.control"
        ],
        "settings": {
          "foreground": "#cba6f7"
        }
      },
      {
        "scope": [
          "entity.name.tag"
        ],
        "settings": {
          "foreground": "#eba0ac"
        }
      },
      {
        "scope": [
          "entity.other.attribute-name.class.css"
        ],
        "settings": {
          "foreground": "#a6e3a1"
        }
      },
      {
        "scope": [
          "string.json.comments",
          "support.type.property-name.json.comments",
          "support.type.property-name.json punctuation"
        ],
        "settings": {
          "foreground": "#d8c4f1dd"
        }
      }
    ]
  },
